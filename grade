grade = int(input("Введіть кількість балів (0-100): "))

if 0 <= grade <= 49:
    print("Оцінка: Незадовільно")
elif 50 <= grade <= 69:
    print("Оцінка: Задовільно")
elif 70 <= grade <= 89:
    print("Оцінка: Добре")
elif 90 <= grade <= 100:
    print("Оцінка: Відмінно")
else:
    print("Помилка! Введіть число від 0 до 100.")
